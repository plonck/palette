{
  "[java]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.formatDocument": "never",
    "source.organizeImports": "never",
    "source.removeUnusedImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.rulers": [80],
  "files.associations": {
    ".editorconfig": "ini",
    "*.yml.template": "yaml",
    "gradlew": "shellscript",
    "HEADER": "plaintext"
  },
  "gradle.nestedProjects": true,
  "gradle.allowParallelRun": true,
  "java.autobuild.enabled": false,
  "java.configuration.updateBuildConfiguration": "interactive",
  "java.gradle.buildServer.enabled": "off",
  "java.import.generatesMetadataFilesAtProjectRoot": false,
  "java.import.gradle.enabled": true,
  "java.import.gradle.wrapper.enabled": true,
  "java.inlayHints.parameterNames.enabled": "none",
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx4G -Xms2G -Xlog:disable",
  "java.saveActions.cleanup": true,
  "java.compile.nullAnalysis.mode": "disabled",
  "java.completion.guessMethodArguments": "off",
  "java.cleanup.actions": [
    "addDeprecated",
    "addFinalModifier",
    "addOverride",
    "redundantModifiers",
    "redundantSuperCall",
    "renameFileToType"
  ],
  "prettier.configPath": "prettier.config.cjs",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "prettier.requireConfig": true,
  "prettier.resolveGlobalModules": false,
  "search.exclude": {
    "**./cache": true,
    "**/.gradle": true,
    "**/.husky/_": true,
    "**/.pnp.*": true,
    "**/.yarn": true,
    "**/*.lock": true,
    "**/build": true,
    "**/gradlew.bat": true,
    "**/gradlew": true,
    "**/node_modules": true
  }
}
